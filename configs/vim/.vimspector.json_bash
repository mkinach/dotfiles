{
  "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
  "configurations": {
    "Run current script (Bash)": {
      "adapter": "vscode-bash",
      "configuration": {
        "request": "launch",
        "program": "${file}",
        "cwd": "${fileDirname}",
        "args": [ "*${args}" ]
      }
    }
  }
}
